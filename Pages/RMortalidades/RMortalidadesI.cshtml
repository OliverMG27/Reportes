@page
@using Reportes.Models;
@model Reportes.Pages.RMortalidades.RMortalidadesIModel
@{
    ViewData["Title"] = "Mortalidades";
}



<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://getbootstrap.com/docs/5.3/assets/css/docs.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>

</head>
<body>


    <div class="container">
        <div class="row">
            <!-- Col 1-->
            <div class="col-12 col-md-2 col-lg-2"> </div>
            <div class="col-12 col-md-2 col-lg-2 text-center"></div>
            <div class="col-12 col-md-4 col-lg-4 text-center">

                <h2 style="font-size: 27px;"><strong>Mortalidades</strong></h2>
                <h2><strong></strong></h2>

            </div>
            <div class="col-12 col-md-2 col-lg-2 text-center">
            </div>
            <div class="col-12 col-md-2 col-lg-2">
                <div class="dropdown">

                    <button type="button" class="btnOp dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-piggy-bank-fill" viewBox="0 0 16 16">
                            <path d="M7.964 1.527c-2.977 0-5.571 1.704-6.32 4.125h-.55A1 1 0 0 0 .11 6.824l.254 1.46a1.5 1.5 0 0 0 1.478 1.243h.263c.3.513.688.978 1.145 1.382l-.729 2.477a.5.5 0 0 0 .48.641h2a.5.5 0 0 0 .471-.332l.482-1.351c.635.173 1.31.267 2.011.267.707 0 1.388-.095 2.028-.272l.543 1.372a.5.5 0 0 0 .465.316h2a.5.5 0 0 0 .478-.645l-.761-2.506C13.81 9.895 14.5 8.559 14.5 7.069c0-.145-.007-.29-.02-.431.261-.11.508-.266.705-.444.315.306.815.306.815-.417 0 .223-.5.223-.461-.026a.95.95 0 0 0 .09-.255.7.7 0 0 0-.202-.645.58.58 0 0 0-.707-.098.735.735 0 0 0-.375.562c-.024.243.082.48.32.654a2.112 2.112 0 0 1-.259.153c-.534-2.664-3.284-4.595-6.442-4.595Zm7.173 3.876a.565.565 0 0 1-.098.21.704.704 0 0 1-.044-.025c-.146-.09-.157-.175-.152-.223a.236.236 0 0 1 .117-.173c.049-.027.08-.021.113.012a.202.202 0 0 1 .064.199Zm-8.999-.65a.5.5 0 1 1-.276-.96A7.613 7.613 0 0 1 7.964 3.5c.763 0 1.497.11 2.18.315a.5.5 0 1 1-.287.958A6.602 6.602 0 0 0 7.964 4.5c-.64 0-1.255.09-1.826.254ZM5 6.25a.75.75 0 1 1-1.5 0 .75.75 0 0 1 1.5 0Z" />
                        </svg>
                    </button>

                    <ul class="dropdown-menu">
                        <li>
                            <a class="dropdown-item" href="/Menu/MenuI">
                                Menú
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-house" viewBox="0 0 16 16">
                                    <path d="M8.707 1.5a1 1 0 0 0-1.414 0L.646 8.146a.5.5 0 0 0 .708.708L2 8.207V13.5A1.5 1.5 0 0 0 3.5 15h9a1.5 1.5 0 0 0 1.5-1.5V8.207l.646.647a.5.5 0 0 0 .708-.708L13 5.793V2.5a.5.5 0 0 0-.5-.5h-1a.5.5 0 0 0-.5.5v1.293L8.707 1.5ZM13 7.207V13.5a.5.5 0 0 1-.5.5h-9a.5.5 0 0 1-.5-.5V7.207l5-5 5 5Z" />
                                </svg>
                            </a>
                        </li>

                        <li>
                            <a class="dropdown-item" href="/Index">
                                Cerrar sesion
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-box-arrow-in-left" viewBox="0 0 16 16">
                                    <path fill-rule="evenodd" d="M10 3.5a.5.5 0 0 0-.5-.5h-8a.5.5 0 0 0-.5.5v9a.5.5 0 0 0 .5.5h8a.5.5 0 0 0 .5-.5v-2a.5.5 0 0 1 1 0v2A1.5 1.5 0 0 1 9.5 14h-8A1.5 1.5 0 0 1 0 12.5v-9A1.5 1.5 0 0 1 1.5 2h8A1.5 1.5 0 0 1 11 3.5v2a.5.5 0 0 1-1 0v-2z" />
                                    <path fill-rule="evenodd" d="M4.146 8.354a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L5.707 7.5H14.5a.5.5 0 0 1 0 1H5.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3z" />
                                </svg>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    </br>
    </br>


    <div class="container mt-4">
        <div class="row">

            @* Col 2 *@
            <div class="col-md-12 col-sm-12 mt-2">
                <br />
            </div>


            <form method="post">
            
             <div class="row">

                 <!-- Col 3 -->
                <div class="col-md-4 col-sm-8 mt-2">
                    <label for="empresas"><strong>Empresa:</strong></label>
                </div>
                @* Col 3 *@
                <div class="col-md-4 col-sm-2 mt-2">
                    <select id="empresas" asp-for="SelectedEmpresaId" asp-items="Model.Empresas">
                    </select>
                </div>
                    @* Col 3 *@
                 <div class="col-md-4 col-sm-2 mt-2">
                    <button type="submit" class="btn-Granja">Cargar Granjas</button>
                </div>
 
             </div>              

            </form>

            @if (Model.Granjas.Count > 0)
            {
                <!-- Col 4 -->
                <div class="col-md-4 col-sm-12 mt-2">
                    <label for="granjas"><strong>Granja:</strong></label>
                </div>
                @* Col 4 *@
                <div class="col-md-8 col-sm-12 mt-2">
                    <select id="granjas" asp-for="SelectedGranjaId" asp-items="Model.Granjas">
                    </select>
                </div>

            }


            <!-- Col 5 -->
            <div class="col-md-4 col-sm-12 mt-2">
                <label for="fechaInicio"><strong>Fechas de entrada:</strong></label>
            </div>
            @* Col 5 *@
            <div class="col-md-8 col-sm-12 mt-2">
                <input id="fechaInicio" type="date" class="custom-date-input" />
            </div>


            <!-- Col 6 -->
            <div class="col-md-4 col-sm-12 mt-2">
                <label for="fechaFinal"><strong>Fechas de salida:</strong></label>
            </div>
            <div class="col-md-8 col-sm-12 mt-2">
                <input id="fechaFinal" type="date" class="custom-date-input" />
            </div>


            <!-- Col 7 -->
            <div class="col-md-12 col-sm-12 mt-2">
                <br />
            </div>
            <!-- Col 8 -->
            <div class="col-md-4 col-sm-12 mt-2">
                <button onclick="ejecutarConsulta()" type="submit" class="btn btn-primary">Mortalidades por semana</button>
            </div>


            @* Col 9 *@
            <div class="col-md-4 col-sm-12 mt-2">
            </div>
            @* Col 10 *@
            <div class="col-md-4 col-sm-12 mt-2">
                <button type="submit" class="btn btn-primary">Mortalidades por concepto</button>
            </div>




            <!-- Col 11 -->
            <div class="col-md-12 col-sm-12 mt-2">
                <br />
            </div>


            <!-- Col 12 -->
            <div class="col-md-4 col-sm-12 mt-2">
                <button type="z" class="btnExcell btn-success">
                    Excel
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-filetype-xlsx" viewBox="0 0 16 16">
                        <path fill-rule="evenodd" d="M14 4.5V11h-1V4.5h-2A1.5 1.5 0 0 1 9.5 3V1H4a1 1 0 0 0-1 1v9H2V2a2 2 0 0 1 2-2h5.5L14 4.5ZM7.86 14.841a1.13 1.13 0 0 0 .401.823c.13.108.29.192.479.252.19.061.411.091.665.091.338 0 .624-.053.858-.158.237-.105.416-.252.54-.44a1.17 1.17 0 0 0 .187-.656c0-.224-.045-.41-.135-.56a1.002 1.002 0 0 0-.375-.357 2.028 2.028 0 0 0-.565-.21l-.621-.144a.97.97 0 0 1-.405-.176.37.37 0 0 1-.143-.299c0-.156.061-.284.184-.384.125-.101.296-.152.513-.152.143 0 .266.023.37.068a.624.624 0 0 1 .245.181.56.56 0 0 1 .12.258h.75a1.093 1.093 0 0 0-.199-.566 1.21 1.21 0 0 0-.5-.41 1.813 1.813 0 0 0-.78-.152c-.293 0-.552.05-.777.15-.224.099-.4.24-.527.421-.127.182-.19.395-.19.639 0 .201.04.376.123.524.082.149.199.27.351.367.153.095.332.167.54.213l.618.144c.207.049.36.113.462.193a.387.387 0 0 1 .153.326.512.512 0 0 1-.085.29.558.558 0 0 1-.255.193c-.111.047-.25.07-.413.07-.117 0-.224-.013-.32-.04a.837.837 0 0 1-.249-.115.578.578 0 0 1-.255-.384h-.764Zm-3.726-2.909h.893l-1.274 2.007 1.254 1.992h-.908l-.85-1.415h-.035l-.853 1.415H1.5l1.24-2.016-1.228-1.983h.931l.832 1.438h.036l.823-1.438Zm1.923 3.325h1.697v.674H5.266v-3.999h.791v3.325Zm7.636-3.325h.893l-1.274 2.007 1.254 1.992h-.908l-.85-1.415h-.035l-.853 1.415h-.861l1.24-2.016-1.228-1.983h.931l.832 1.438h.036l.823-1.438Z" />
                    </svg>
                </button>
             
            </div>


            @* Col 13 *@
            <div class="col-md-4 col-sm-12 mt-2">
            </div>


            @* Col 14 *@
            <div class="col-md-4 col-sm-12 mt-2">
                <button type="button" class="btnPDF btn-danger">PDF
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-file-pdf-fill" viewBox="0 0 16 16">
                        <path d="M5.523 10.424c.14-.082.293-.162.459-.238a7.878 7.878 0 0 1-.45.606c-.28.337-.498.516-.635.572a.266.266 0 0 1-.035.012.282.282 0 0 1-.026-.044c-.056-.11-.054-.216.04-.36.106-.165.319-.354.647-.548zm2.455-1.647c-.119.025-.237.05-.356.078a21.035 21.035 0 0 0 .5-1.05 11.96 11.96 0 0 0 .51.858c-.217.032-.436.07-.654.114zm2.525.939a3.888 3.888 0 0 1-.435-.41c.228.005.434.022.612.054.317.057.466.147.518.209a.095.095 0 0 1 .026.064.436.436 0 0 1-.06.2.307.307 0 0 1-.094.124.107.107 0 0 1-.069.015c-.09-.003-.258-.066-.498-.256zM8.278 4.97c-.04.244-.108.524-.2.829a4.86 4.86 0 0 1-.089-.346c-.076-.353-.087-.63-.046-.822.038-.177.11-.248.196-.283a.517.517 0 0 1 .145-.04c.013.03.028.092.032.198.005.122-.007.277-.038.465z" />
                        <path fill-rule="evenodd" d="M4 0h8a2 2 0 0 1 2 2v12a2 2 0 0 1-2 2H4a2 2 0 0 1-2-2V2a2 2 0 0 1 2-2zm.165 11.668c.09.18.23.343.438.419.207.075.412.04.58-.03.318-.13.635-.436.926-.786.333-.401.683-.927 1.021-1.51a11.64 11.64 0 0 1 1.997-.406c.3.383.61.713.91.95.28.22.603.403.934.417a.856.856 0 0 0 .51-.138c.155-.101.27-.247.354-.416.09-.181.145-.37.138-.563a.844.844 0 0 0-.2-.518c-.226-.27-.596-.4-.96-.465a5.76 5.76 0 0 0-1.335-.05 10.954 10.954 0 0 1-.98-1.686c.25-.66.437-1.284.52-1.794.036-.218.055-.426.048-.614a1.238 1.238 0 0 0-.127-.538.7.7 0 0 0-.477-.365c-.202-.043-.41 0-.601.077-.377.15-.576.47-.651.823-.073.34-.04.736.046 1.136.088.406.238.848.43 1.295a19.707 19.707 0 0 1-1.062 2.227 7.662 7.662 0 0 0-1.482.645c-.37.22-.699.48-.897.787-.21.326-.275.714-.08 1.103z" />
                    </svg>
                </button>
            </div>
        </div>
    </div>
 
@section scripts {
    <script>

        //Id Empresa
            var selectElement = document.getElementById('empresas');
            selectElement.addEventListener('change', function () {
                var selectedValue = selectElement.value;
                console.log('Empresa seleccionada: ', selectedValue);
            });


        //Id Granja
            var selectElement2 = document.getElementById('granjas');
            selectElement2.addEventListener('change', function () {
                var selectedValue2 = selectElement2.value;
                console.log('Granja Select: ', selectedValue2);
            });


        //Fecha entrada
            var inputFecha = document.getElementById('FechaEntrada');
            inputFecha.addEventListener('change', function () {
                console.log('Fecha Entrada:', inputFecha.value);
            });


        //Fecha Salida
            var inputFecha2 = document.getElementById('FechaSalida');
            inputFecha2.addEventListener('change', function () {
                console.log('Fecha Salida:', inputFecha2.value);
            });

    </script>
}
</body>
</html>









<style>


    /* Estilos boton */
    .btn {
        color: white;
        background-color: #92002d;
        text-align: center;
        padding: 10px 20px;
        line-height: 12px;
    }

    .btnOp {
        color: white;
        background-color: #00288D;
        text-align: center;
        padding: 10px 20px;
        line-height: 12px;
        transform: translate(-50%, -50%);
        position: relative;
        border-radius: 10px;
        margin-top: 18px;
    }

    .btn-Granja{
        border-radius: 10px;
    }
        

    .btnExcell{
        border-radius: 10px;

    }

    .btnPDF{
        border-radius: 10px;
    }


</style>